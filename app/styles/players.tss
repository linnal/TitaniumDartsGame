"#sv":{
	scrollType: "horizontal",
	width: "100%",
	height: "100%"
}

".sv_views":{
	layout: "vertical",
	left: 0,
	right:0,
	top:0,
	bottom:0
}

"#v_intro":{
	left: 0,
	right:0,
	height: "50dp",
	backgroundColor: "#334D5C"
}

"#lbl_intro":{
	top: "20dp",
	left:"20dp",
	font:{
		fontSize: "20dp",
		fontWeight: "white"
	},
	color: "white"
}

"#b_trash":{
	right: "20dp",
	width : "40dp",
	height: "40dp",
	backgroundImage: "/trash_idle.png",
	select: false
}

"#listView":{ 
	height: "64%",
	separatorColor: "transparent"
}

"#template":{
	height: "70dp"
}

"#row": {
	height: "70dp"
}

"#pic":{
	backgroundImage: "/icon_img.png", 
	width: "70dp",
	height: "70dp",
